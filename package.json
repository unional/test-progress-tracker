{
  "name": "test-progress-tracker",
  "version": "0.0.0-development",
  "description": "Keep track of test progress over time",
  "homepage": "https://github.com/unional/test-progress-tracker",
  "bugs": {
    "url": "https://github.com/unional/test-progress-tracker/issues"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/unional/test-progress-tracker.git"
  },
  "license": "MIT",
  "main": "dist/index.js",
  "typings": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "yarn clean && tsc -p tsconfig.base.json",
    "clean": "rimraf dist",
    "codacy-coverage": "cat ./coverage/lcov.info | codacy-coverage",
    "coverage": "jest --coverage",
    "dependency-check": "dependency-check . --unused --no-dev && dependency-check . --missing --no-dev",
    "lint": "eslint --ext=ts,js .",
    "test": "tsc && jest",
    "verify": "yarn lint && yarn build && yarn coverage && yarn dependency-check",
    "verify-ci": "yarn lint && yarn build && jest --coverage --config jest.config.ci.json && yarn dependency-check",
    "watch": "jest --watch"
  },
  "dependencies": {
    "chokidar": "^3.5.2",
    "global-store": "^1.0.0-beta.19",
    "mkdirp": "^1.0.4",
    "unpartial": "^0.6.4"
  },
  "devDependencies": {
    "@types/jest": "^27.0.3",
    "@types/mkdirp": "^1.0.2",
    "@types/node": "^16.11.11",
    "@types/rimraf": "^3.0.2",
    "assertron": "^7.0.2",
    "delay": "^5.0.0",
    "dependency-check": "^4.1.0",
    "eslint": "^8.4.0",
    "eslint-plugin-harmony": "^6.0.0",
    "jest": "^27.4.3",
    "jest-progress-tracker": "^3.0.0",
    "jest-validate": "^27.4.2",
    "jest-watch-repeat": "^2.0.0",
    "jest-watch-suspend": "^1.1.2",
    "jest-watch-toggle-config": "^2.0.1",
    "rimraf": "^3.0.0",
    "ts-jest": "^27.1.0",
    "typescript": "^4.5.2"
  }
}
